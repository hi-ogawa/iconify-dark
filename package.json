    "@types/express": "^4.17.17",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@vavite/connect": "^1.8.3",
    "@vitejs/plugin-react": "^4.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "vite": "^4.4.7"
